<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jgwhit14.cs450finalproject">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MapsActivity" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".MyLocations" />
        <activity android:name=".Friends" />
        <activity android:name=".MyFriendsLocations" />
        <activity android:name=".SaveLocation"
            android:theme="@style/Base.Theme.AppCompat.Dialog"></activity>
        <activity android:name=".SelectLocation"
            android:theme="@style/Base.Theme.AppCompat.Dialog"></activity>
    </application>

</manifest>